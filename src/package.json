{
  "name": "watson-ml",
  "version": "0.1.0",
  "scripts": {
    "client-remove": "cd client && yarn remove",
    "client-add": "cd client && yarn add",
    "client": "cd client && yarn start",
    "build": "cd client && npm run build",
    "deploy": "export DEPLOY_TO=production && ./deploy.sh",
    "stage": "export DEPLOY_TO=staging && ./deploy.sh",
    "server": "nodemon server.js",
    "test": "echo successssssss",
    "start": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "localbuild": "NODE_ENV=localbuild PORT=3000 node server.js"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "frameguard": "^3.1.0",
    "request": "^2.88.2",
    "socket.io": "^2.4.0",
    "socket.io-redis": "^5.2.0",
    "spdy": "^4.0.1",
    "ws": "^7.2.1"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.2"
  }
}
